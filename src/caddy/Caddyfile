{
    # email your-email@example.com
}

{$BACKEND_DOMAIN} {
	reverse_proxy backend:8000
}

{$FRONTEND_DOMAIN} {
    # Reverse proxy for the Frontend
    reverse_proxy frontend:3000 {
        @notfound {
			status 404
		}
        handle_response @notfound {
            copy_response {
				status 200
			}
        }
    }
}
